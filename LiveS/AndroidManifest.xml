<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lives"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/home"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.lives.ScrenHomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.lives.SearchLiveActivity"
            android:label="Recherche d'un live" >
        </activity>
        <activity
            android:name="com.example.lives.SeeLiveActivity"
            android:label="Voir mes lives" >
        </activity>
        <activity
            android:name="com.example.lives.AddLiveActivity"
            android:label="Ajouter un Live" >
        </activity>
        <activity
            android:name="com.example.lives.ResulteSearch"
            android:label="resultat de recherche" >
        </activity>
        <activity
            android:name="com.example.lives.ResulteSearchActivity"
            android:label="Resultat de recherche" >
        </activity>
        <activity
            android:name="com.example.lives.DetailLivesActivity"
            android:label="detail d'un live" >
        </activity>
        <activity
            android:name="com.example.lives.DetailLiveUserActivity"
            android:label="modifier mon live" >
        </activity>
    </application>

</manifest>